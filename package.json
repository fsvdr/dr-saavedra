  "description": "Personal portfolio and blog for Dr. Benito Saavedra Alvarado",
  "version": "1.0.0",
  "dependencies": {
    "gatsby": "^2.3.25",
    "gatsby-image": "^2.0.39",
    "gatsby-plugin-manifest": "^2.0.29",
    "gatsby-plugin-offline": "^3.2.23",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-source-filesystem": "^2.0.29",
    "gatsby-transformer-sharp": "^2.1.18",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "css-mqpacker": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.1.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-postcss": "^2.1.0",
    "gatsby-plugin-stylelint": "^3.2.0",
    "gatsby-remark-images": "^3.1.2",
    "gatsby-transformer-remark": "^2.5.0",
    "husky": ">=4",
    "lint-staged": "^10.2.11",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.1"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "lint": "eslint --ignore-path .gitignore .",
    "check-format": "prettier *.js --list-different",
    "prettier": "prettier *.js --ignore-path .gitignore",
    "format": "prettier src/ --write --ignore-path .gitignore",
    "validate": "lint && build",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "volta": {
    "node": "12.18.3",
    "yarn": "1.22.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint"
    ]
  }
